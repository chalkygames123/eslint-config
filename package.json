{
  "name": "@chalkygames123/eslint-config",
  "version": "3.0.0",
  "publishConfig": {
    "access": "public"
  },
  "description": "Default ESLint config used by @chalkygames123",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chalkygames123/eslint-config.git"
  },
  "author": "Takuya Fukuju <chalkygames123@gmail.com>",
